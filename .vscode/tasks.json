{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "make",
			"command": "make",
			"args": [
				"-f",
				"mymake"
			],
			"options": {
				"cwd": "${workspaceFolder}",
			},
			"presentation": {
				"clear": true,
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
		},
	]
}