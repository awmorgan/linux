{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "make",
			"command": "time",
			"args": [
				"lkmake",
				"ARCH=arm64",
				"CC=clang",
				"CROSS_COMPILE=aarch64-linux-gnu-",
				"-j8",
				"2>&1",
				"|tee",
				".make.log",
			],
			"options": {
				"cwd": "${workspaceFolder}",
			},
			"presentation": {
				"clear": true,
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
		},
	]
}